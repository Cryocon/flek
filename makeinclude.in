exec_prefix	=@exec_prefix@
bindir		=@bindir@
includedir	=@includedir@
libdir		=@libdir@
srcdir		=@srcdir@
VPATH		=@srcdir@

# compiler names:
CXX		=@CXX@
CC		=@CC@
MAKEDEPEND	=@MAKEDEPEND@

# flags for C++ compiler:
CFLAGS		=@CFLAGS@
CXXFLAGS	=@CXXFLAGS@

FLTKDIR = @FLTKDIR@
FLTKLIB = @FLTKLIB@
FLTKINC = @FLTKINC@

LIBNAME		=@LIBNAME@
DSOLIBNAME	=@DSOLIBNAME@
LIBCOMMAND	=@LIBCOMMAND@
RANLIB		=@RANLIB@
DSOEXT		=@DSOEXT@
DSOCOMMAND	=@DSOCOMMAND@

# libraries to link with:
LDLIBS		=@LDFLAGS@ @LIBS@ -L/usr/X11R6/lib $(FLTKLIB) -lfltk -lX11 -lXext @X_EXTRA_LIBS@ -lm

